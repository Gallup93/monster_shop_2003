<h1>Please enter your information</h1>
<br>
<br>
<section align = "center">
  <%= form_tag '/users', method: :post do %>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
    <%= label_tag :address %>
    <%= text_field_tag :address %>
    <%= label_tag :city %>
    <%= text_field_tag :city %>
    <%= label_tag :state %>
    <%= text_field_tag :state %>
    <%= label_tag :zip %>
    <%= text_field_tag :zip %>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <%= submit_tag "Create User" %>
  <% end %>
<section>
